{
    "class": "AS3",
    "action": "deploy",
    "declaration": {
      "class": "ADC",
      "schemaVersion": "3.0.0",
      "id": "UDP_DNS_Sample",
      "{{tennant_name}}": {
        "class": "Tenant",
        "{{VS}}": {
          "class": "Application",
          "{{VS}}": {
            "class": "Service_UDP",
            "virtualPort": 53,
            "virtualAddresses": [
              "192.168.1.190"
            ],
            "pool": "{{pool}}"
          },
          "{{pool}}": {
            "class": "Pool",
            "monitors": [
              "icmp"
            ],
            "members": [
              {
                "servicePort": 53,
                "serverAddresses": [
                  "8.8.8.8",
                  "8.8.4.4"
                ]
              }
            ]
          }
        }
      }
    }
   }